<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<import resource="user-msg.xml"/>

	<bean id="myTestBean" class="com.example.test.ch2.MyTestBean"/>
	<bean name="people" class="com.example.test.ch2.People"/>

	<bean id="student" class="com.example.test.ch2.Student">
		<constructor-arg ref="user"/>
		<constructor-arg value="80"/>
		<constructor-arg ref="people"/>
	</bean>

</beans>